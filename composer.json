{
    "name": "mathieutu/laravel-pdflayer",
    "description": "A pdflayer API bridge for Laravel",
    "type": "package",
    "require": {
        "php": ">=5.6",
        "laravel/framework": "^5.1",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mathieu TUDISCO",
            "email": "dev@mathieutu.ovh"
        }
    ],
    "autoload": {
        "psr-4": {
            "MathieuTu\\PDFLayer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MathieuTu\\PDFLayer\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
